Cell[TextData[{
 "In dimensionless units it corresponds to the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"p", "+", 
           RowBox[{"\[ScriptQ]", "[", "1", "]"}]}], ")"}], "2"], "+", "1"}], 
       ")"}], 
      RowBox[{"-", "1"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"p", "+", 
           RowBox[{"\[ScriptQ]", "[", "1", "]"}], "+", 
           RowBox[{"\[ScriptQ]", "[", "2", "]"}]}], ")"}], "2"], "+", "1"}], 
       ")"}], 
      RowBox[{"-", "1"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"p", "+", 
           RowBox[{"\[ScriptQ]", "[", "1", "]"}], "+", 
           RowBox[{"\[ScriptQ]", "[", "2", "]"}], "+", 
           RowBox[{"\[ScriptQ]", "[", "3", "]"}]}], ")"}], "2"], "+", "1"}], 
       ")"}], 
      RowBox[{"-", "1"}]], 
     RowBox[{
      SuperscriptBox["\[DifferentialD]", "d"], "p"}]}]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"9bc63979-7979-4136-99a2-25be5259ea97"],
 "."
}], "Notes",
 CellChangeTimes->{{3.880806922553261*^9, 3.880807126288046*^9}, {
  3.880808872238719*^9, 3.880808880584879*^9}, {3.88130391934048*^9, 
  3.881303975921118*^9}, {3.884936860360613*^9, 3.884936873283339*^9}, {
  3.884936907193643*^9, 3.884936940307082*^9}, {3.884937371282548*^9, 
  3.8849373736089163`*^9}},
 CellID->1801230407,ExpressionUUID->"b68a3962-3f5f-4a1f-bec8-0d487ae004e0"],

Cell[TextData[{
 "In ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "=", "3"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "286a35e4-091a-4d55-b2e0-7f769e518c3d"],
 " the integrated subdiagram corresponds to the function ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     SuperscriptBox["tan", 
      RowBox[{"-", "1"}]], "(", 
     FractionBox[
      RowBox[{" ", 
       SqrtBox[
        RowBox[{
         RowBox[{
          SuperscriptBox["q1", "2"], " ", 
          SuperscriptBox["q2", "2"], " ", 
          SuperscriptBox["q3", "2"]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"q1", "+", "q2", "-", "q3"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{"q1", "-", "q2", "+", "q3"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "q1"}], "+", "q2", "+", "q3"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{"q1", "+", "q2", "+", "q3"}], ")"}]}]}]]}], 
      RowBox[{"8", "+", 
       SuperscriptBox["q1", "2"], "+", 
       SuperscriptBox["q2", "2"], "+", 
       SuperscriptBox["q3", "2"]}]], ")"}], 
    RowBox[{"4", " ", "\[Pi]", " ", 
     SqrtBox[
      RowBox[{
       RowBox[{
        SuperscriptBox["q1", "2"], " ", 
        SuperscriptBox["q2", "2"], " ", 
        SuperscriptBox["q3", "2"]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"q1", "+", "q2", "-", "q3"}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{"q1", "-", "q2", "+", "q3"}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "q1"}], "+", "q2", "+", "q3"}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{"q1", "+", "q2", "+", "q3"}], ")"}]}]}]]}]], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "d040670b-a765-4f6c-8442-4f082e25b1f2"],
 " with ",
 Cell[BoxData[
  StyleBox["q1", "TI"]], "InlineFormula",ExpressionUUID->
  "d062b0a9-75a4-4674-bb91-1ddf9a915717"],
 ", ",
 Cell[BoxData[
  StyleBox["q2", "TI"]], "InlineFormula",ExpressionUUID->
