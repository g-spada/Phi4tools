Cell["The subdiagram is regularized by zero-momentum subtraction.", "Notes",
 CellChangeTimes->{{3.881658250178244*^9, 3.881658270051675*^9}},
 CellID->1091382638,ExpressionUUID->"64f6f2c7-dd55-4aba-b7c4-f09254863ce9"],

Cell[TextData[{
 "In dimensionless units it corresponds to the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalSigma]", "(", "q", ")"}], "-", 
    RowBox[{"\[CapitalSigma]", "(", "0", ")"}], " "}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"9c9c732e-4957-44fc-b876-11e902ccc47b"],
 ", with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalSigma]", "(", "q", ")"}], " ", "=", 
    RowBox[{"\[Integral]", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["p", "2"], "+", "1"}], ")"}], 
       RowBox[{"-", "1"}]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["k", "2"], "+", "1"}], ")"}], 
       RowBox[{"-", "1"}]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"p", "+", "q", "+", "k"}], ")"}], "2"], "+", "1"}], ")"}], 
       
       RowBox[{"-", "1"}]], 
      RowBox[{
       SuperscriptBox["\[DifferentialD]", "d"], "p"}], "  ", 
      RowBox[{
       SuperscriptBox["\[DifferentialD]", "d"], "k"}]}]}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "6325cf80-91c5-4e7e-a82e-eef625a54b40"],
 "."
}], "Notes",
 CellChangeTimes->{{3.880806922553261*^9, 3.880807126288046*^9}, {
  3.880808872238719*^9, 3.880808880584879*^9}, {3.88130391934048*^9, 
  3.881303975921118*^9}, {3.881658348608593*^9, 3.881658376405218*^9}, {
  3.881658416609182*^9, 3.881658471277401*^9}, {3.885741229002737*^9, 
  3.885741229002821*^9}},
 CellID->1801230407,ExpressionUUID->"8d9d17e9-05d8-499d-8178-f4228db9d762"],

Cell[TextData[{
 "In ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "=", "3"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "06c5e154-40ca-485b-8c76-5b0e14337e20"],
 " the integrated subdiagram corresponds to the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", 
     FractionBox["1", 
      RowBox[{"32", " ", 
       SuperscriptBox["\[Pi]", "2"]}]]}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"log", "(", 
       RowBox[{
        FractionBox[
         SuperscriptBox["q", "2"], "9"], "+", "1"}], ")"}], "+", 
      FractionBox[
       RowBox[{"6", " ", 
        RowBox[{
         SuperscriptBox["tan", 
          RowBox[{"-", "1"}]], "(", 
         FractionBox["q", "3"], ")"}]}], "x"], "-", "2"}], ")"}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "57c9d91f-23da-4cff-9670-693bbb2b56ac"],
 " with ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]], "InlineMath",ExpressionUUID->
  "b3a98bfd-ab5a-4091-a2ee-5612109b7107"],
 " the modulus of the three-dimensional momentum vector."
}], "Notes",
 CellChangeTimes->{{3.881303978768105*^9, 3.881304017978354*^9}, {
  3.881304152813613*^9, 3.881304214838736*^9}, {3.8816593894859943`*^9, 
