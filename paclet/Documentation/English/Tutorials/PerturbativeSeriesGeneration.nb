Cell[BoxData[
 RowBox[{
  RowBox[{"betaSymbolic", "[", "o_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"order", "=", "o"}], ",", "\[CapitalGamma]2ns", ",", 
      "\[CapitalGamma]2", ",", "d\[CapitalGamma]2", ",", "\[CapitalGamma]4", 
      ",", "m", ",", "g", ",", "Z", ",", "Zg", ",", "gofgP", ",", "ZP", ",", 
      "ZgP", ",", "\[Beta]series"}], "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[CapitalGamma]2", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["m", "2"], 
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"Sum", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"a2", "[", "n", "]"}], 
              SuperscriptBox["g", "n"]}], ",", 
             RowBox[{"{", 
              RowBox[{"n", ",", "2", ",", "order"}], "}"}]}], "]"}]}], 
          ")"}]}], ")"}], "+", 
       SuperscriptBox[
        RowBox[{"O", "[", "g", "]"}], 
        RowBox[{"order", "+", "1"}]]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"d\[CapitalGamma]2", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"a2d", "[", "n", "]"}], 
            SuperscriptBox["g", "n"]}], ",", 
           RowBox[{"{", 
            RowBox[{"n", ",", "2", ",", "order"}], "}"}]}], "]"}]}], ")"}], 
       "+", 
       SuperscriptBox[
        RowBox[{"O", "[", "g", "]"}], 
        RowBox[{"order", "+", "1"}]]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[CapitalGamma]4", "=", 
      RowBox[{
       RowBox[{"m", " ", "g", 
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"a4", "[", 
              RowBox[{"n", "+", "1"}], "]"}], 
             SuperscriptBox["g", "n"]}], ",", 
            RowBox[{"{", 
             RowBox[{"n", ",", "1", ",", "order"}], "}"}]}], "]"}]}], ")"}]}],
        "+", 
       SuperscriptBox[
        RowBox[{"O", "[", "g", "]"}], 
        RowBox[{"order", "+", "1"}]]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Z", "=", 
      RowBox[{
       RowBox[{"(", 
        FractionBox["1", "d\[CapitalGamma]2"], ")"}], "//", "Simplify"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Zg", "=", 
      RowBox[{
       FractionBox[
        RowBox[{"m", " ", "g"}], "\[CapitalGamma]4"], "//", "Simplify"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"gofgP", "=", 
      RowBox[{
       RowBox[{"InverseSeries", "[", 
        RowBox[{
         RowBox[{"Simplify", "[", 
          RowBox[{
           FractionBox["\[CapitalGamma]4", 
            SqrtBox[
             RowBox[{
              SuperscriptBox["d\[CapitalGamma]2", "3"], "*", 
              "\[CapitalGamma]2"}]]], ",", 
           RowBox[{"m", ">", "0"}]}], "]"}], ",", "gP"}], "]"}], "//", 
       "Simplify"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"ZP", "=", 
      RowBox[{
       RowBox[{"Z", "/.", 
        RowBox[{"g", "->", "gofgP"}]}], "//", "Simplify"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ZgP", "=", 
      RowBox[{
       RowBox[{"Zg", "/.", 
        RowBox[{"g", "->", "gofgP"}]}], "//", "Simplify"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Beta]series", "=", 
      RowBox[{"-", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["\[PartialD]", "gP"], 
          RowBox[{"Log", "[", 
           FractionBox[
            RowBox[{"gP", "*", "ZgP"}], 
            SuperscriptBox["ZP", "2"]], "]"}]}], ")"}], 
        RowBox[{"-", "1"}]]}]}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

