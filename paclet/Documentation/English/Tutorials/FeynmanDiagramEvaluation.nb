            SubscriptBox[
             RowBox[{"\[ScriptQ]", "[", "2", "]"}], "\"\<\[Rho]\>\""], "2"], 
           RowBox[{"2", 
            SuperscriptBox["\[Pi]", "2"]}]], 
          FractionBox[
           RowBox[{"Sin", "[", 
            SubscriptBox[
             RowBox[{"\[ScriptQ]", "[", "2", "]"}], "\"\<\[Theta]\>\""], 
