   RowBox[{"l_", ",", "intgr_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"intsimpl", ",", "out"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Which", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"l", "==", "0"}], ",", "intgr", ",", "\[IndentingNewLine]", 
      RowBox[{"l", "==", "1"}], ",", 
      RowBox[{
       RowBox[{"intsimpl", "=", 
        RowBox[{"Simplify", "[", 
         RowBox[{"intgr", " ", 
          FractionBox[
           SuperscriptBox[
            SubscriptBox[
             RowBox[{"\[ScriptQ]", "[", "1", "]"}], "\"\<\[Rho]\>\""], "2"], 
           RowBox[{"2", 
            SuperscriptBox["\[Pi]", "2"]}]]}], "]"}]}], ";", 
       RowBox[{"NIntegrate", "[", 
        RowBox[{"intsimpl", ",", 
