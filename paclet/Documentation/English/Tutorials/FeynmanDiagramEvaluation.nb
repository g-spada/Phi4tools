Cell[CellGroupData[{

Cell[TextData[{
 "Let's consider, as an example, the diagrams of the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[CapitalGamma]", 
     RowBox[{"(", "4", ")"}]], "(", "0", ")"}], TraditionalForm]],
  ExpressionUUID->"fd3991e5-73a1-42ad-96b4-c5694518dd23"],
 " at the order ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Lambda]", "4"], TraditionalForm]],ExpressionUUID->
  "4e1287bf-9183-42b7-bbc1-2803b31c317b"],
 ", i.e. with 4 quartic vertices with zero external momenta."
}], "Text",
 CellChangeTimes->{3.8835437908994837`*^9},
 CellID->781738226,ExpressionUUID->"137993c0-26ed-44a0-8fa4-354fc7b5c540"],

